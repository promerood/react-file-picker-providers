{
  "name": "react-file-picker-providers",
  "version": "1.7.0",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Pablo David Romero <pablo.romeropablo@gmail.com>",
  "files": [
    "build"
  ],
  "license": "MIT License",
  "keywords": [
    "components",
    "react",
    "react-file-picker-providers",
    "DropBox",
    "Google Drive"
  ],
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "file-loader": "2.0.0",
    "file-saver": "^1.3.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "load-script": "^1.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "build:scss": "cpx \"./src/library/**/*.scss\" ./lib",
    "prebuild:lib": "cross-env rimraf lib",
    "build:lib": "npm-run-all --parallel build:commonjs build:scss build:copy-files",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "deploy:docs": "npm run build:docs",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/library --out-dir ./lib --ignore spec.js",
    "version": "npm run build:lib",
    "postversion": "git push && git push --tags && cd lib/ && npm publish"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "base-href-webpack-plugin": "^2.0.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "chokidar": "^3.2.2",
    "copy-webpack-plugin": "^4.6.0",
    "cpx": "^1.5.0",
    "cross-env": "^4.0.0",
    "css-loader": "^3.2.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-formatter-pretty": "^2.0.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "event-stream": "^4.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fs-extra": "3.0.1",
    "highlight.js": "^9.13.1",
    "html-webpack-plugin": "3.2.0",
    "interpolate-html-plugin": "^3.0.0",
    "jest": "^25.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.6",
